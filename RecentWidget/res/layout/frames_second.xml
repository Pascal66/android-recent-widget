			android:background="@drawable/widget_label_box" android:textSize="12dip"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_gravity="bottom|center_horizontal" android:maxLines="2"
			android:ellipsize="end" android:gravity="center_horizontal" />

	</FrameLayout>

